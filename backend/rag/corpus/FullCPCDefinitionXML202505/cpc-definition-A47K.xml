<table-row><table-column><paragraph-text type="body">Nozzles; Spray heads</paragraph-text></table-column><table-column><paragraph-text type="body"><class-ref scheme="cpc">B05B</class-ref></paragraph-text></table-column></table-row>
<table-row><table-column preferred-width="13.19cm"><paragraph-text type="body">Wash stands for automobiles</paragraph-text></table-column><table-column preferred-width="4.19cm"><paragraph-text type="body"><class-ref scheme="cpc">B60R</class-ref></paragraph-text></table-column></table-row>
