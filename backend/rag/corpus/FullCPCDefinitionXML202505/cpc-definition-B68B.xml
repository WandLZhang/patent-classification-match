<?xml version="1.0" encoding="UTF-8"?>
<definitions publication-date="2016-11-01" publication-type="official">
<definition-item><classification-symbol scheme="cpc">B68B</classification-symbol><definition-title>HARNESS; DEVICES USED IN CONNECTION THEREWITH; WHIPS OR THE LIKE</definition-title><definition-statement>
      <section-title>Definition statement</section-title>
      <section-body><paragraph-text type="preamble">This place covers:</paragraph-text>
        <paragraph-text type="body">Harnesses and devices used in connection therewith that are suitable for horses or other traction animals; whips in general, e.g. horsewhips, riding crops.</paragraph-text>
      </section-body>
    </definition-statement><references><section-title>References</section-title><limiting-references><section-title>Limiting references</section-title><section-body><paragraph-text type="preamble">This place does not cover:</paragraph-text>
        <table><table-row>
            <table-column preferred-width="11.2cm">
              <paragraph-text type="body">Leads or collars for other animals</paragraph-text>
            </table-column>
            <table-column preferred-width="6.18cm">
              <paragraph-text type="body"><class-ref scheme="cpc">A01K27/00</class-ref></paragraph-text>
            </table-column>
          </table-row>
          
        </table>
      </section-body></limiting-references></references></definition-item>
<definition-item><classification-symbol scheme="cpc">B68B1/00</classification-symbol><definition-title>Devices in connection with harness, for hitching, reining, training, breaking or quietening horses or other traction animals  (devices of this kind combined with traction harness <class-ref scheme="cpc">B68B3/00</class-ref>; stable equipment <class-ref scheme="cpc">A01K1/00</class-ref>; devices forming part of a vehicle <class-ref scheme="cpc">B62C</class-ref>)</definition-title><definition-statement>
      <section-title>Definition statement</section-title>
      <section-body><paragraph-text type="preamble">This place covers:</paragraph-text>
        <paragraph-text type="body">Halters; bridles; reins; bits; stranglers; blinkers; devices for preventing bolting; hobbling devices; vaulting harnesses; longeing equipment.</paragraph-text>
      </section-body>
    </definition-statement><references><section-title>References</section-title><limiting-references><section-title>Limiting references</section-title><section-body><paragraph-text type="preamble">This place does not cover:</paragraph-text>
        <table><table-row>
            <table-column preferred-width="11.2cm">
              <paragraph-text type="body">Devices of this kind combined with a traction harness</paragraph-text>
            </table-column>
            <table-column preferred-width="6.18cm">
              <paragraph-text type="body"><class-ref scheme="cpc">B68B3/00</class-ref></paragraph-text>
            </table-column>
          </table-row><table-row>
            <table-column preferred-width="11.2cm">
              <paragraph-text type="body">Devices for unhitching bolting traction animals</paragraph-text>
            </table-column>
            <table-column preferred-width="6.18cm">
              <paragraph-text type="body"><class-ref scheme="cpc">B68B5/08</class-ref></paragraph-text>
            </table-column>
          </table-row><table-row>
            <table-column preferred-width="11.2cm">
              <paragraph-text type="body">Stable equipment</paragraph-text>
            </table-column>
            <table-column preferred-width="6.18cm">
              <paragraph-text type="body"><class-ref scheme="cpc">A01K</class-ref></paragraph-text>
            </table-column>
          </table-row><table-row>
            <table-column preferred-width="11.2cm">
              <paragraph-text type="body">Devices for preventing the animal from cribbing</paragraph-text>
            </table-column>
            <table-column preferred-width="6.18cm">
              <paragraph-text type="body"><class-ref scheme="cpc">A01K13/00</class-ref></paragraph-text>
            </table-column>
          </table-row><table-row>
            <table-column preferred-width="11.2cm">
              <paragraph-text type="body">Devices forming part of a vehicle</paragraph-text>
            </table-column>
            <table-column preferred-width="6.18cm">
              <paragraph-text type="body"><class-ref scheme="cpc">B62C</class-ref></paragraph-text>
            </table-column>
          </table-row>
          
          
          
          
          
        </table>
      </section-body></limiting-references></references></definition-item>
<definition-item><classification-symbol scheme="cpc">B68B3/00</classification-symbol><definition-title>Traction harnesses; Traction harnesses combined with devices referred to in group <class-ref scheme="cpc">B68B1/00</class-ref></definition-title><definition-statement>
      <section-title>Definition statement</section-title>
      <section-body><paragraph-text type="preamble">This place covers:</paragraph-text>
        <paragraph-text type="body">Yokes; horse collars; manufacturing of horse collars; locks for horse collars; breast collars; cruppers; breechings; traces; trace holders.</paragraph-text>
      </section-body>
    </definition-statement><references><section-title>References</section-title><limiting-references><section-title>Limiting references</section-title><section-body><paragraph-text type="preamble">This place does not cover:</paragraph-text>
