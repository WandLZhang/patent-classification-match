<definitions publication-date="2016-11-01" publication-type="official">
<definition-item><classification-symbol scheme="cpc">G09D</classification-symbol><definition-title>RAILWAY OR LIKE TIME OR FARE TABLES; PERPETUAL CALENDARS  (calendar blocks <class-ref scheme="cpc">B42D5/04</class-ref>; clockwork driven <class-ref scheme="cpc">G04B</class-ref>; comprising computing means <class-ref scheme="cpc">G06C</class-ref>)</definition-title><definition-statement>
      <section-title>Definition statement</section-title>
      <section-body><paragraph-text type="preamble">This place covers:</paragraph-text>
        <paragraph-text type="body">Railway tables or similar time/fare tables.</paragraph-text>
        <paragraph-text type="body">Indicating or reading aids for said tables.</paragraph-text>
        <paragraph-text type="body">Perpetual calendars.</paragraph-text>
      </section-body>
    </definition-statement><references><section-title>References</section-title><limiting-references><section-title>Limiting references</section-title><section-body><paragraph-text type="preamble">This place does not cover:</paragraph-text>
        <table><table-row>
            <table-column preferred-width="12.05cm">
              <paragraph-text type="body">Calendar blocks in printed format</paragraph-text>
            </table-column>
            <table-column preferred-width="2.96cm">
              <paragraph-text type="body"><class-ref scheme="cpc">B42D5/04</class-ref></paragraph-text>
            </table-column>
          </table-row>
          
        </table>
      </section-body></limiting-references><informative-references>
