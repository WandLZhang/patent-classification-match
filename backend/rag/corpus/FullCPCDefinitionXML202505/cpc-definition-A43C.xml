<definition-item><classification-symbol scheme="cpc">A43C13/00</classification-symbol><definition-title>Wear-resisting attachments</definition-title><definition-statement>
         <section-title>Definition statement</section-title>
         <section-body><paragraph-text type="preamble">This place covers:</paragraph-text>
            <paragraph-text type="body">Additional device(s) attached on the shoe to protect against wear.</paragraph-text>
         </section-body>
      </definition-statement></definition-item>
