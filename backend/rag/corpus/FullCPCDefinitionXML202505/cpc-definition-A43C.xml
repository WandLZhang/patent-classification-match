<definitions publication-date="2016-11-01" publication-type="official">
<definition-item><classification-symbol scheme="cpc">A43C</classification-symbol><definition-title>FASTENINGS OR ATTACHMENTS OF FOOTWEAR; LACES IN GENERAL</definition-title><definition-statement>
         <section-title>Definition statement</section-title>
         <section-body><paragraph-text type="preamble">This place covers:</paragraph-text>
            <paragraph-text type="body">Equipment of the shoe e.g. laces, eyelets, cleats, studs.</paragraph-text>
         </section-body>
      </definition-statement><references><section-title>References</section-title><limiting-references><section-title>Limiting references</section-title><section-body><paragraph-text type="preamble">This place does not cover:</paragraph-text>
            <table><table-row>
                  <table-column preferred-width="9.89cm">
                     <paragraph-text type="body">Equipment for ski boots</paragraph-text>
                  </table-column>
                  <table-column preferred-width="5.14cm">
                     <paragraph-text type="body"> <class-ref scheme="cpc">A43B5/0415</class-ref> </paragraph-text>
                  </table-column>
               </table-row><table-row>
                  <table-column preferred-width="9.89cm">
                     <paragraph-text type="body">Equipment for skate</paragraph-text>
                  </table-column>
                  <table-column preferred-width="5.14cm">
                     <paragraph-text type="body"> <class-ref scheme="cpc">A43B5/16</class-ref> </paragraph-text>
                  </table-column>
               </table-row>
               
               
            </table>
         </section-body></limiting-references></references></definition-item>
<definition-item><classification-symbol scheme="cpc">A43C1/00</classification-symbol><definition-title>Shoe lacing fastenings  (garment fastening devices <class-ref scheme="cpc">A41F</class-ref>)</definition-title><definition-statement>
         <section-title>Definition statement</section-title>
         <section-body><paragraph-text type="preamble">This place covers:</paragraph-text>
            <paragraph-text type="body">Every fastening system including at least one lace and concerning the lace, e.g. a special way of lacing, a specific position of the lace on the shoe, several laces used, etc.</paragraph-text>
         </section-body>
      </definition-statement><references><section-title>References</section-title><limiting-references><section-title>Limiting references</section-title><section-body><paragraph-text type="preamble">This place does not cover:</paragraph-text>
            <table><table-row>
                  <table-column preferred-width="10.14cm">
                     <paragraph-text type="body">Hooks for laces; Guards for hooks</paragraph-text>
                  </table-column>
                  <table-column preferred-width="4.89cm">
                     <paragraph-text type="body"> <class-ref scheme="cpc">A43C3/00</class-ref> </paragraph-text>
                  </table-column>
               </table-row><table-row>
                  <table-column preferred-width="10.14cm">
                     <paragraph-text type="body">Eyelets</paragraph-text>
                  </table-column>
                  <table-column preferred-width="4.89cm">
                     <paragraph-text type="body"> <class-ref scheme="cpc">A43C5/00</class-ref> </paragraph-text>
                  </table-column>
               </table-row><table-row>
                  <table-column preferred-width="10.14cm">
                     <paragraph-text type="body">Holding devices for laces</paragraph-text>
                  </table-column>
                  <table-column preferred-width="4.89cm">
                     <paragraph-text type="body"> <class-ref scheme="cpc">A43C7/00</class-ref> </paragraph-text>
                  </table-column>
               </table-row><table-row>
                  <table-column preferred-width="10.14cm">
                     <paragraph-text type="body">Laces; Laces in general for garments made of textiles, leather, or plastics</paragraph-text>
                  </table-column>
                  <table-column preferred-width="4.89cm">
                     <paragraph-text type="body"> <class-ref scheme="cpc">A43C9/00</class-ref> </paragraph-text>
                  </table-column>
               </table-row><table-row>
                  <table-column preferred-width="10.14cm">
                     <paragraph-text type="body">Cable fastening</paragraph-text>
                  </table-column>
                  <table-column preferred-width="4.89cm">
                     <paragraph-text type="body"> <class-ref scheme="cpc">A43C11/16</class-ref> </paragraph-text>
                  </table-column>
               </table-row>
               
               
               
               
               
